<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/activity_main2"
    style="@style/NavPage"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="@dimen/activity_vertical_margin">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <com.yuntongxun.plugin.common.common.base.CCPClearEditText
            android:id="@+id/ed01"
            style="@style/CCPEditText"
            android:layout_width="match_parent"
            android:hint="@string/ytx_please_entry_friend_account"
            android:inputType="number" />

        <EditText
            android:id="@+id/ed02"
            style="@style/CCPEditText"
            android:layout_width="match_parent"
            android:layout_marginTop="@dimen/NormalPadding"
            android:hint="@string/ytx_please_entry_friend_account"
            android:inputType="number" />

        <EditText
            android:id="@+id/ed03"
            style="@style/CCPEditText"
            android:layout_width="match_parent"
            android:layout_marginTop="@dimen/NormalPadding"
            android:hint="@string/ytx_please_entry_friend_account"
            android:inputType="number" />

        <EditText
            android:id="@+id/ed04"
            style="@style/CCPEditText"
            android:layout_width="match_parent"
            android:layout_marginTop="@dimen/NormalPadding"
            android:hint="@string/ytx_please_entry_friend_account"
            android:inputType="number" />

        <EditText
            android:id="@+id/ed05"
            style="@style/CCPEditText"
            android:layout_width="match_parent"
            android:layout_marginTop="@dimen/NormalPadding"
            android:hint="@string/ytx_please_entry_friend_account"
            android:inputType="number" />

        <Button
            android:id="@+id/chattingBtn"
            style="@style/CCPBigGreenButton"
            android:layout_marginTop="@dimen/NormalPadding"
            android:text="@string/ytx_create_group" />

    </LinearLayout>

</RelativeLayout>
